@using System.Web.Optimization
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
        @Styles.Render("~/bundles/css")
        @Styles.Render("~/bundles/chameleon-bootstrapcss")
        <!--[if lt IE 9]>
            @Scripts.Render("~/bundles/html5shiv")
        <![endif]-->
    </head>
    <body>
        <header class="navbar navbar-default" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".basil-navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/" class="navbar-brand">Knockout vs Chameleon Forms</a>
                </div>
                <nav class="collapse navbar-collapse">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="/">Ding ding</a></li>
                    </ul>
                </nav>
            </div>
        </header>
        
        
        <div class="container">
            @RenderBody()
        </div>
    
        @Scripts.Render("~/bundles/js")
        @Scripts.Render("~/bundles/chameleon-bootstrapjs");
        @RenderSection("script", required: false)

    </body>
</html>
